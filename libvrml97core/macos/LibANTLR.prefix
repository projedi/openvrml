#include <stdlib.h>
#include <iostream.h>

#define ANTLR_C_USING(x)	using std:: ## x
#define NO_TEMPLATE_PARTS

extern void exit();

inline int strcasecmp(const char *s1, const char *s2)
{
  while (true) {
    char c1 = tolower(*s1++),
    c2 = tolower(*s2++);
    if (c1 < c2) return -1;
    if (c1 > c2) return 1;
    if (c1 == 0) return 0;
  }
}
